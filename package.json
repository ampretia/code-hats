{
  "name": "code-hats",
  "version": "0.0.1",
  "description": "code-hats",
  "license": "MIT",
  "repository": "",
  "author": "mbw",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "clean": "rimraf lib",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "build": "npm run format && npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive \"test/**/*-spec.ts\"",
    "coverage": "nyc --include=\"src/**/*.ts\" --reporter=text --reporter=html --reporter=lcov mocha --compilers ts:ts-node/register --recursive \"test/**/*-spec.ts\"",
    "docker": "docker build . -t calanais/code-hats"
  },
  "dependencies": {
    "@types/debug": "^4.1.4",
    "@types/express": "^4.17.0",
    "@types/express-ws": "^3.0.0",
    "@types/markdown-it": "0.0.8",
    "@types/nunjucks": "^3.1.1",
    "@types/pty.js": "^0.2.35",
    "@types/socket.io": "^2.1.2",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "markdown-it": "^9.0.1",
    "nunjucks": "^3.2.0",
    "pty.js": "^0.3.1",
    "socket.io": "^2.2.0",
    "xterm": "3.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.13",
    "chai": "^4.2.0",
    "coveralls": "^3.0.5",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}
